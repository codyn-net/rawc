node "n"
{
    q' = "dq"
    dq = 0
}

edge on "n" {
    dq <= "t"
}

node "c"
{
    q = "[0, 1]" | integrated ("-0.5", "0.5")
    q' = "2 * pi * sin(t * 2 * pi) + (1 - q)"

    lala = "q + 2" | out
}

# Test loop with multidim indices
node "n{1:3}"
{
    q = "[1; 1]]"
    q' = "0.5 + [q[1]; q[0]]"
}

